package components



templ Title(s string) {
    @TitleStyle()
	<h1 class="title">{ s }</h1>
}

templ TitleStyle() {
    <style>
        .title {
            color: var(--color-white);
            font-size: 70px;
            font-weight: bold;
            margin-bottom: 12px;
            margin-top: 0px;
        }
    </style>
    
}